convertToHMchar <- function(input) {
if (nchar(input == 4)) {
output <- paste0(str_extract(input, pattern = "^.."),
":",
str_extract(input, pattern = "..$"))
} else if (nchar(input) == 3) {
output <- paste0(str_extract(input, pattern = "^."),
":",
str_extract(input, pattern = "..$"))
}
}
